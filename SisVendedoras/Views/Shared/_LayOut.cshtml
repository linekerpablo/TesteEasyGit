<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Andrea Modas</title>
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <link href="~/assets/css/bootstrap.min.css" rel="stylesheet" />
    <link href="~/assets/css/font-awesome.min.css" rel="stylesheet" />
    <link href="~/assets/css/please-wait.css" rel="stylesheet" />
    <script src="~/assets/js/please-wait.min.js"></script>
    <script src="~/assets/js/modernizr.js"></script>
    <script src="~/assets/js/jquery.js"></script>
    <script src="~/scripts/angular.min.js"></script>
    <script src="~/scripts/toastr.min.js"></script>
    <link href="~/Content/Style.css" rel="stylesheet" />

    <script src="~/scripts/app/app.js"></script>
    <script src="~/scripts/app/marca/MarcaController.js"></script>
    <script src="~/scripts/app/marca/MarcaServico.js"></script>

    <link href="~/Content/angular-toastr.css" rel="stylesheet" />
    <script src="~/scripts/angular-animate.js"></script>
    <script src="~/scripts/toastr.js"></script>
    <script src="~/scripts/angular-toastr.tpls.js"></script>

    <link href="~/bower_components/angular-data-grid/dist/angular-data-grid.bootstrap.css" rel="stylesheet" />
    <script src="~/bower_components/angular-data-grid/dist/pagination.min.js"></script>
    <script src="~/bower_components/angular-data-grid/dist/dataGrid.min.js"></script>

    <link href="~/bower_components/angular-loading-bar/src/loading-bar.css" rel="stylesheet" />
    <script src="~/bower_components/angular-loading-bar/src/loading-bar.js"></script>

    <script src="~/scripts/utilidades/utilidades.js"></script>
    <!--[if lt IE 9]>
    <script src="~/assets/js/html5shiv.min.js"></script>
    <script src="~/assets/js/respond.min.js"></script>
    <![endif]-->
</head>
<!-- ADD THE CLASS fixed TO GET A FIXED HEADER AND SIDEBAR LAYOUT -->
<!-- the fixed layout is not compatible with sidebar-mini -->
<body class="hold-transition skin-blue fixed sidebar-mini" ng-app="app">
    <!-- Site wrapper -->
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>A</b>Modas</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>Andrea</b>Modas</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="offcanvas" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @Html.Partial("Profile")
                    </ul>
                </div>
            </nav>
        </header>
        <!-- =============================================== -->
        <aside class="main-sidebar">
            @Html.Partial("Menus")
        </aside>
        <!-- =============================================== -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            <section class="content-header">
                <h1>
                    Seja bem vindo ao sistema para vendedoras
                </h1>
            </section>
            <!-- Main content -->
            <section class="content">
                @RenderBody()
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <div class="pull-right hidden-xs">
                <b>Version</b> @ViewContext.Controller.GetType().Assembly.GetName().Version
            </div>
            <strong>Copyright &copy; @DateTime.Now.Year <a href="https://www.cwsit.com.br">CWS.IT</a>.</strong>
        </footer>
    </div>
    <!-- ./wrapper -->
    <link href="~/assets/css/AdminLTE.min.css" rel="stylesheet" />
    <link href="~/assets/css/skin-blue.min.css" rel="stylesheet" />
    <link href="~/assets/plugins/iCheck/flat/blue.css" rel="stylesheet" />
    <script src="~/assets/js/bootstrap.min.js"></script>
    <script src="~/assets/js/jquery.validate.min.js"></script>
    <script src="~/assets/plugins/slimScroll/jquery.slimscroll.min.js"></script>
    <script src="~/assets/plugins/fastclick/fastclick.min.js"></script>
    <script src="~/assets/js/app.min.js"></script>
    <script src="~/assets/js/demo.js"></script>
    <script src="~/assets/js/AdminLTE.js"></script>
    <script src="~/assets/plugins/iCheck/icheck.min.js"></script>
    <script>
        $(document).ready(function () {
            $('input[type="radio"]')
                .iCheck({
                    checkboxClass: 'icheckbox_flat-blue',
                    radioClass: 'iradio_flat-blue',
                    increaseArea: '20%' // optional
                });

            $('input[type="checkbox"]:not(.bootstrap-switch)')
                .iCheck({
                    checkboxClass: 'icheckbox_flat-blue',
                    radioClass: 'iradio_flat-blue',
                    increaseArea: '20%' // optional
                });
        })
    </script>
    @RenderSection("scripts", required: false)
    @{
        if (ViewBag.Erro != null)
        {
            foreach (var m in (List<string>)ViewBag.Erro)
            {
                <script>
                    MsgErro('@m');
                </script>
            }
        }

        if (ViewBag.Ok != null)
        {
            foreach (var m in (List<string>)ViewBag.Ok)
            {
                <script>
                    MsgOk('@m');
                </script>
            }
        }

        if (ViewBag.Info != null)
        {
            foreach (var m in (List<string>)ViewBag.Info)
            {
                <script>
                    MsgInfo('@m');
                </script>
            }
        }

        if (ViewBag.Alerta != null)
        {
            foreach (var m in (List<string>)ViewBag.Alerta)
            {
                <script>
                    MsgAlerta('@m');
                </script>
            }
        }

    }
</body>
</html>
